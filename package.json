{
  "name": "proposal-billing-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "create-dummy-users": "npx tsx scripts/create-dummy-users.ts",
    "dev": "next dev",
    "build": "prisma generate && prisma db push --accept-data-loss || (echo 'Migration failed, continuing build...' && prisma generate && next build)",
    "postbuild": "node scripts/copy-pdfkit-fonts.js",
    "start": "next start",
    "lint": "next lint",
    "postinstall": "prisma generate || echo 'Prisma generate failed in postinstall'",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:migrate:prod": "npx tsx scripts/migrate-db.ts",
    "db:studio": "prisma studio",
    "db:migrate-upfront": "prisma db push && prisma generate"
  },
  "dependencies": {
    "@neondatabase/serverless": "^0.10.0",
    "@prisma/adapter-neon": "^5.22.0",
    "@prisma/client": "^5.22.0",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@sparticuz/chromium": "^131.0.0",
    "@types/papaparse": "^5.5.2",
    "bcryptjs": "^2.4.3",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "date-fns": "^3.6.0",
    "geist": "^1.5.1",
    "lucide-react": "^0.427.0",
    "next": "^14.2.0",
    "next-auth": "^4.24.7",
    "papaparse": "^5.5.3",
    "pdfkit": "^0.17.2",
    "postcss-js": "^5.0.3",
    "puppeteer": "^24.34.0",
    "puppeteer-core": "^24.34.0",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "resend": "^4.0.0",
    "sonner": "^1.4.0",
    "tailwind-merge": "^2.5.2",
    "tailwindcss-animate": "^1.0.7",
    "use-debounce": "^10.0.6",
    "ws": "^8.18.0",
    "xlsx": "^0.18.5",
    "zod": "^3.23.8"
